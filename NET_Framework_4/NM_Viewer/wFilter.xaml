<Window x:Class="NM_Viewer.wFilter"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NM_Viewer"
        mc:Ignorable="d" ResizeMode="NoResize"
        Title="Filter" Height="120" Width="200">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Orientation="Vertical">
            <CheckBox x:Name="chkTiploc" IsChecked="True" Content="Tiplocs"/>
            <CheckBox x:Name="chkSignals" IsChecked="False" Content="Signals"/>
            <CheckBox x:Name="chkELOCS" IsChecked="False" Content="ELOCS"/>
        </StackPanel>
        <Button Grid.Row="1" x:Name="btnGo" Content="Load" Click="BtnGo_OnClick"></Button>

    </Grid>
</Window>
